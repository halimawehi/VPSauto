#!/bin/sh

# Change Squid Page
cat > /usr/share/squid-langpack/en/ERR_INVALID_URL <<-END
<html><head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<title>Welcome to PinoyThread</title>
<script type="text/javascript">
function Redirect()
{
 window.location="https://www.pinoythread.com";
}
setTimeout('Redirect()', 10);
</script>
<center>blah blah blah</center><br><br>
<hr>
<div id="footer">
<p>SQUID PROXY Generated by %h on %T (%s) Philippines</p>
<!-- %c -->
</div>
</body></html>
END
# Success test
cd
echo "Success"
